{
  "name": "vue-template-mobile",
  "version": "1.0.0",
  "description": "vue template mobile",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "analyz": "vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0",
    "nprogress": "^0.2.0",
    "number-precision": "^1.3.1",
    "vant": "2.0.0",
    "vee-validate": "^2.2.0",
    "vue": "^2.6.8",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-e2e-cypress": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-unit-mocha": "^3.5.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chai": "^4.1.2",
    "chalk": "^2.4.2",
    "compression-webpack-plugin": "^2.0.0",
    "conventional-changelog-cli": "^2.0.21",
    "cz-conventional-changelog": "^2.1.0",
    "cz-customizable": "^6.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.4",
    "minimist": "^1.2.0",
    "node-sass": "^4.11.0",
    "postcss-px2rem": "^0.3.0",
    "sass-loader": "^7.1.0",
    "style-resources-loader": "^1.2.1",
    "svg-sprite-loader": "^4.1.3",
    "url-loader": "^1.1.2",
    "vue-server-renderer": "^2.6.8",
    "vue-skeleton-webpack-plugin": "^1.2.2",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "^0.14.11",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-merge": "^4.2.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "resolutions": {
    "webpack-dev-middleware": "3.6.0"
  },
  "lint-staged": {
    "linters": {
      "*.{js,vue}": [
        "vue-cli-service lint",
        "git add"
      ]
    },
    "ignore": [
      "package.json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
